{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules", "dist"],
  "verbose": true,
  "execMap": {
    "ts": "node"
  },

  "runOnChangeOnly": false,
  "watch": ["src/**/**/*.ts", "dist/service/index.js", "dist/gateway.js"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,ts,json,gql"
}
